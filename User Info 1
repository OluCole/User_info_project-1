first_name = input("enter your first name: ")
m_i = input("enter your middle initial: ")
last_name = input("enter your last name: ")
s_a = input("enter street adress: ")
city_location = input("enter city: ")
zip_code = input("enter state zipcode: ")
ph_n = input("enter phone number: ")
email_ad = input("enter email: ")

print(f"{first_name} {m_i} {last_name}")
print(f"{s_a}")
print(f"{city_location}, {zip_code}")
print(f"{ph_n}")
print(f"{email_ad}")
